{
  "name": "DevTools",
  "version": "1.0",
  "manifest_version": 2,
  "description": "A tab override chrome extension for useful links and sources that we use everyday as web developers.",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Very Useful Links",
    "default_popup": "extension.html"
  },

  "chrome_url_overrides" : {
    "newtab": "extension.html"
  },
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
  "icons": {
    "128": "icon.png"
  }
}